        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:FirmaTransportowa.Views"
        mc:Ignorable="d"
        Title="DodajZastosowaniePojazdu" Height="183" Width="301">
    <Grid Margin="0,0,2,1">
        <Label Content="Zastosowanie:" Height="26" FontSize="11" Margin="10,24,-10,75"/>
        <TextBox Height="26" Width="120" FontSize="9" Margin="96,24,26,69" Name="Zastosowanie"/>
        <Button Content="OK" Height="20" Width="80" Margin="32,74,130,25" Click="AddDestination"/>
